.container
  %h4 Edit About Us page

  = simple_form_for :about, url: about_path(@about), method: :patch do |f|
    .form-inputs
      .row-fluid
        .col-sm-12
          = f.input :content, as: :summernote

    .form-actions
      = f.button :submit, "Save", class: "btn-danger"
      = link_to 'Cancel', :back, class: "btn btn-default"